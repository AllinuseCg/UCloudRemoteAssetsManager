<Window x:Class="UCloudRemoteAssetsManager.LogWindowSpace.logWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:UCloudRemoteAssetsManager.LogWindowSpace"
        mc:Ignorable="d"
        Title="log" Height="800" Width="400">

    <ScrollViewer x:Name="scrollView" HorizontalScrollBarVisibility="Visible">
        <ListBox x:Name="logBox" ItemsSource="{Binding}"/>
    </ScrollViewer>
</Window>
